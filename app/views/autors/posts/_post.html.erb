<div id="<%= dom_id post %>">
  <p>
    <strong>Titulo:</strong>
    <%= post.titulo %>
  </p>

  <p>
    <strong>Titulo descripcion:</strong>
    <%= post.titulo_descripcion %>
  </p>

  <p>
    <strong>Publicado:</strong>
    <%= post.publicado %>
  </p>

  <p>
    <strong>Publicado el:</strong>
    <%= post.publicado_el %>
  </p>

  <p>
    <strong>Autor:</strong>
    <%= post.autor_id %>
  </p>

</div>
